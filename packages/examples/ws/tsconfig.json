{
  "compilerOptions": {
    "target": "ES6",
    "module": "commonjs",
    "outDir": "./dist",
    "rootDir": "./src",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": ".",
    "paths": {
      "cyberserver": ["node_modules/cyberserver/dist"]
    },

  },
  "include": ["src/**/*.ts"],
  "exclude": ["node_modules"]
}
